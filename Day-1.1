import pandas as pd

# Creating the dataset
data = {
    'Order ID': [1001, 1002, 1003, 1004, 1005, 1006, 1007, 1008, 1009, 1010],
    'Customer ID': ['C001', 'C002', 'C001', 'C003', 'C002', 'C004', 'C005', 'C003', 'C002', 'C004'],
    'Product ID': ['P001', 'P003', 'P002', 'P001', 'P002', 'P003', 'P001', 'P003', 'P001', 'P002'],
    'Quantity': [2, 1, 1, 3, 2, 1, 1, 2, 1, 3],
    'Total Price': [50.00, 30.00, 20.00, 75.00, 40.00, 30.00, 25.00, 60.00, 25.00, 60.00]
}

# Loading the data into a DataFrame
df = pd.DataFrame(data)

# Display the DataFrame
print(df)
total_revenue = df['Total Price'].sum()
print(f"Total Revenue: ${total_revenue:.2f}")
orders_per_customer = df['Customer ID'].value_counts()
print(orders_per_customer)
total_quantity = df['Quantity'].sum()
print(f"Total Quantity of Products Sold: {total_quantity}")
revenue_per_product = df.groupby('Product ID')['Total Price'].sum()
print(revenue_per_product)
popular_product = df.groupby('Product ID')['Quantity'].sum().idxmax()
print(f"Most Popular Product: {popular_product}")
